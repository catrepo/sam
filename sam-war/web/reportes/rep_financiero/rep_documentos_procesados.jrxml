	<queryString>
		<![CDATA[SELECT
tipo_nombre,
doc_numero,
doc_concepto,
doc_valor,
doc_usuasignacion,
doc_dependencia,
doc_comprobante,
(case when doc_revisioncon = 't' then 'Enviado'
when doc_revisiondev = 't' then 'Devuelto'
when doc_revisioncon is null and doc_revisiondev is null then 'Pendiente'
end) as estado
,
doc_fechacon,
doc_observacion
from tes_documentos
inner join tes_tipo_documento on tes_documentos.id_tipo=tes_tipo_documento.id_tipo
where (case when doc_revisioncon = 't' then 'Enviado'
when doc_revisiondev = 't' then 'Devuelto'
when doc_revisioncon is null and doc_revisiondev is null then 'Pendiente'
end) = $P{parametro} or (case when doc_revisioncon = 't' then 'Enviado'
when doc_revisiondev = 't' then 'Devuelto'
when doc_revisioncon is null and doc_revisiondev is null then 'Pendiente'
end)= $P{parametro1}
order by tipo_nombre,estado]]>
	</queryString>
