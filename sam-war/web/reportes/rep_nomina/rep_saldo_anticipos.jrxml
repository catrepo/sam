	<queryString>
		<![CDATA[select fecha_ant,ced_empleado,nom_empleado,(case when contr_empleado = 'NL' then 'Trabajador' else 'Empleado' end) as contr_empleado,valor,cuotas,valor_pagado,valor_saldo,cuota_pagada
,(cast(mes_ini as varchar) + '/' + cast(anio_ini as varchar)) as fecha_ini,(cast(mes_fin as varchar)+ '/' +cast(anio_fin as varchar)) as fecha_fin
