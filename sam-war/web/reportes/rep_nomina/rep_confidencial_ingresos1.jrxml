	<queryString>
		<![CDATA[select * from (
SELECT codtra,valtrs, 1 as id
,(SELECT USRRHH.NODATING.NOLING FROM USRRHH.NODATING where USRRHH.NODATING.CODING =codtrs and USRRHH.NODATING.CODCIA = 'MA')as item
,sum(valtrs) over (PARTITION BY codtra ORDER BY codtra) as total_ingreso
FROM NOTRANOM
where TIPTRS = 'I' and CODPER = $P{mes} and to_char(fecini,'YYYY') = $P{anio}
and CODTRA = $P{codigo})
where id = $P{bandera}]]>
	</queryString>
	<field name="CODTRA" class="java.lang.String"/>
	<field name="VALTRS" class="java.math.BigDecimal"/>
	<field name="ID" class="java.math.BigDecimal"/>
	<field name="ITEM" class="java.lang.String"/>
	<field name="TOTAL_INGRESO" class="java.math.BigDecimal"/>
