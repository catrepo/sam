	<queryString>
		<![CDATA[select * from (
SELECT DISTINCT
s.ide_empleado_solicitante,
s.ci_solicitante,
s.solicitante,
(SELECT
ide_solicitud_anticipo
from srh_solicitud_anticipo
where srh_solicitud_anticipo.ide_empleado_solicitante=s.ide_empleado_solicitante
order by ide_solicitud_anticipo desc limit 1) as id
from srh_solicitud_anticipo s
) as a
inner join
(SELECT fecha_anticipo,valor_anticipo,valor_pagado, (valor_anticipo-valor_pagado)as saldo ,ide_solicitud_anticipo,ide_estado_anticipo
from srh_calculo_anticipo
) as b
on a.id = b.ide_solicitud_anticipo
inner join
(SELECT valor,periodo,anio,ide_anticipo,
(case when ide_estado_cuota = 1 then 'Pagado' else 'Pendiente' end) as ide_estado_cuota,
ide_detalle_anticipo
FROM srh_detalle_anticipo

) as c
on a.id = c.ide_anticipo
where ide_estado_anticipo in (2,3,4)
order by solicitante,ide_detalle_anticipo]]>
	</queryString>
