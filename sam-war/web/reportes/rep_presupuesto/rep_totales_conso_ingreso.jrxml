<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="rep_totales_conso_ingreso" language="groovy" pageWidth="842" pageHeight="595" orientation="Landscape" columnWidth="818" leftMargin="12" rightMargin="12" topMargin="12" bottomMargin="12" uuid="b2d665fa-e26a-4284-a6ec-274b1b5eff7c">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="fechai" class="java.lang.Integer"/>
	<parameter name="fechaf" class="java.lang.Integer"/>
	<parameter name="inicial" class="java.lang.Integer"/>
	<parameter name="reforma" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select inicial1,codificado,reforma1,devengado1,ejecutado1,(codificado-devengado1) as saldo
from (select sum((case when inicial is null then 0 else inicial end ))as inicial1,
sum((case when reforma is null then 0 else reforma end ))as reforma1,
sum((case when codificado is null then 0 else codificado end))as codificado,
sum((case when devengado is null then 0 else devengado end ))as devengado1,
sum((case when ejecutado is null then 0 else ejecutado end ))as ejecutado1
from(select CUENMC as CUENMCC ,nivemc,codificado
from (select CUENMC,nivemc
from USFIMRU.TIGSA_GLM03
where TIPLMC= 'R' and substr(CEDTMC,1,1) in (1,2,3) and NIVEMC = 6)
left join
(SELECT CUENDT,SUM(MONTDT)*-1 as codificado
FROM USFIMRU.TIGSA_GLB01
where STATDT='E'
AND CCIADT<>'MR'
AND SAPRDT>=$P{inicial}
AND SAPRDT<=$P{reforma}
and substr(FDOCDT,1,5) <= $P{fechaf}
GROUP BY CUENDT
order by CUENDT)
on CUENMC = CUENDT)
left join
(select CUENMC as CUENMCI,nivemc,inicial
from (select CUENMC,nivemc
from USFIMRU.TIGSA_GLM03
where TIPLMC= 'R' and substr(CEDTMC,1,1) in (1,2,3) and NIVEMC = 6)
left join
(SELECT CUENDT,SUM(MONTDT)*-1 as inicial
FROM USFIMRU.TIGSA_GLB01
where STATDT='E'
AND CCIADT<>'MR'
AND SAPRDT=$P{inicial}
GROUP BY CUENDT
HAVING SUM(MONTDT)<=0
order by CUENDT)
on CUENMC = CUENDT)
on CUENMCC = CUENMCI
left join
(select CUENMC as CUENMCR ,nivemc,reforma
from (select CUENMC,nivemc
from USFIMRU.TIGSA_GLM03
where TIPLMC= 'R' and substr(CEDTMC,1,1) in (1,2,3) and NIVEMC = 6)
left join
(SELECT CUENDT,SUM(MONTDT)*-1 as reforma
FROM USFIMRU.TIGSA_GLB01
where STATDT='E'
AND CCIADT<>'MR'
AND SAPRDT=$P{reforma}
and substr(FDOCDT,1,5) <= $P{fechaf}
GROUP BY CUENDT
order by CUENDT)
on CUENMC = CUENDT)
on CUENMCC = CUENMCR
left join
(select CUENMC as CUENMCD,NIVEMC,devengado
from (select CUENMC,NIVEMC
from USFIMRU.TIGSA_GLM03
where TIPLMC= 'R' and substr(CEDTMC,1,1) in (1,2,3) and NIVEMC = 6)
left join
(select F01CTD,devengado
from(select CUENDT,inicial
from (select CUENMC,nivemc
from USFIMRU.TIGSA_GLM03
where TIPLMC= 'R' and substr(CEDTMC,1,1) in (1,2,3) and NIVEMC = 6)
left join
(SELECT CUENDT,SUM(MONTDT)*-1 as inicial
FROM USFIMRU.TIGSA_GLB01
where STATDT='E'
AND CCIADT<>'MR'
AND SAPRDT>=$P{inicial}
AND SAPRDT<=$P{reforma}
and substr(FDOCDT,1,5) <= $P{fechaf}
GROUP BY CUENDT
order by CUENDT)
on CUENMC = CUENDT)
inner join
(select F01CTD,devengado
from (SELECT  DISTINCT a.F01CTD,sum(MONTDT*-1) as devengado
FROM USFIMRU.TIGSA_GLB01,
(SELECT  F01CIO
,(case when (case when F01A1O is null then F01A2O else F01A1O end) is null then '0'
 else  (case when F01A1O is null then F01A2O else F01A1O end) end) as F01A1O
,F01CTD,F01CTO
 FROM USFIMRU.ECEF01
WHERE F01A1D='2') a
WHERE STATDT='E'
AND CCIADT=a.F01CIO
AND CUENDT = a.F01CTO
AND (case when CAUXDT is null then '0' else CAUXDT end)  =a.F01A1O
AND SAPRDT>=$P{fechai}
AND SAPRDT<=$P{fechaf}
AND TMOVDT='H'
group by F01CTD)
where F01CTD <> '3801010000'
union
SELECT  DISTINCT a.F01CTD,sum(MONTDT*-1) as devengado
FROM USFIMRU.TIGSA_GLB01,
(SELECT  F01CIO
,(case when (case when F01A1O is null then F01A2O else F01A1O end) is null then '0'
 else  (case when F01A1O is null then F01A2O else F01A1O end) end) as F01A1O
,F01CTD,F01CTO
 FROM USFIMRU.ECEF01
WHERE F01A1D='2') a
WHERE STATDT='E'
AND CCIADT=a.F01CIO
AND CUENDT = a.F01CTO
AND (case when AUAD01 is null then '0' else AUAD01 end)  = a.F01A1O
AND SAPRDT>=$P{fechai}
AND SAPRDT<=$P{fechaf}
AND TMOVDT='H'
group by F01CTD
union
SELECT  DISTINCT a.F01CTD,sum(MONTDT*-1) as devengado
FROM USFIMRU.TIGSA_GLB01,
(SELECT  DISTINCT F01CIO
,F01CTD,F01CTO
 FROM USFIMRU.ECEF01
WHERE F01A1D='2' and F01COO = 'H') a
WHERE STATDT='E'
AND CCIADT=a.F01CIO
AND CUENDT = a.F01CTO
AND SAPRDT>=$P{fechai}
AND SAPRDT<=$P{fechaf}
AND TMOVDT='H'
group by F01CTD)
on CUENDT = F01CTD)
on CUENMC= F01CTD)
on CUENMCC = CUENMCD
left join
(
select CUENMC as CUENMCE,NIVEMC,ejecutado
from (select CUENMC,NIVEMC
from USFIMRU.TIGSA_GLM03
where TIPLMC= 'R' and substr(CEDTMC,1,1) in (1,2,3) and NIVEMC = 6)
left join
(select F01CTD,ejecutado
from(select CUENDT,inicial
from (select CUENMC,nivemc
from USFIMRU.TIGSA_GLM03
where TIPLMC= 'R' and substr(CEDTMC,1,1) in (1,2,3) and NIVEMC = 6)
left join
(SELECT CUENDT,SUM(MONTDT)*-1 as inicial
FROM USFIMRU.TIGSA_GLB01
where STATDT='E'
AND CCIADT<>'MR'
AND SAPRDT>=$P{inicial}
AND SAPRDT<=$P{reforma}
and substr(FDOCDT,1,5) <= $P{fechaf}
GROUP BY CUENDT
order by CUENDT)
on CUENMC = CUENDT)
inner join
(select F01CTD,devengado as ejecutado
from (SELECT  DISTINCT a.F01CTD,sum(MONTDT*-1) as devengado
FROM USFIMRU.TIGSA_GLB01,
(SELECT  F01CIO
,(case when (case when F01A1O is null then F01A2O else F01A1O end) is null then '0'
 else  (case when F01A1O is null then F01A2O else F01A1O end) end) as F01A1O
,F01CTD,F01CTO
 FROM USFIMRU.ECEF01
WHERE F01A1D='3') a
WHERE STATDT='E'
AND CCIADT=a.F01CIO
AND CUENDT = a.F01CTO
AND (case when CAUXDT is null then '0' else CAUXDT end)  =a.F01A1O
AND SAPRDT>=$P{fechai}
AND SAPRDT<=$P{fechaf}
AND TMOVDT='H'
group by F01CTD)
where F01CTD <> '3801010000'
union
select F01CTD,devengado
from (SELECT  DISTINCT a.F01CTD,sum(MONTDT*-1) as devengado
FROM USFIMRU.TIGSA_GLB01,
(SELECT  DISTINCT F01CIO
,F01CTD,F01CTO
 FROM USFIMRU.ECEF01
WHERE F01A1D='3' and F01COO = 'H') a
WHERE STATDT='E'
AND CCIADT=a.F01CIO
AND CUENDT = a.F01CTO
AND SAPRDT>=$P{fechai}
AND SAPRDT<=$P{fechaf}
AND TMOVDT='H'
group by F01CTD)
where F01CTD = '3801010000' or F01CTD = '3602010000')
on CUENDT = F01CTD)
on CUENMC= F01CTD
order by  CUENMC)
on CUENMCC = CUENMCE)]]>
	</queryString>
