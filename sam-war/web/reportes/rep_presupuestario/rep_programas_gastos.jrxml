select SUBSTR(CUENDT, 1, 1) as CUENDT , POSTMA , SUM(reforma) as reforma  from (
SELECT CUENDT , AUAD02 , SUM(MONTDT) as reforma
FROM USFIMRU.TIGSA_GLB01
where STATDT='E'
AND CCIADT='CM'
AND SAPRDT=$P{reforma}
GROUP BY CUENDT, AUAD02
having AUAD02 is not null)
left join
(SELECT CAUXMA,POSTMA FROM USFIMRU.TIGSA_GLM05 where TAUXMA = 'CC'
UNION
SELECT CAUXMA,POSTMA FROM USFIMRU.TIGSA_GLM05 where TAUXMA = 'PY')
on AUAD02 = CAUXMA
GROUP BY SUBSTR(CUENDT, 1, 1),POSTMA)
GROUP BY CUENDT,POSTMA)
on CUENDT=CUENDTR and POSTMA = POSTMAR)
on CUENMC = CUENDT
where nivemc  between $P{niveli} and $P{nivelf} and POSTMA = $P{programa}
order by CUENMC]]>
