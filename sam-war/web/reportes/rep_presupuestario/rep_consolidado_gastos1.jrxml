	<queryString>
		<![CDATA[select * from(
select  CUENMC,partida,NOLAAD,sum(inicial) as inicial,sum(reforma)as reforma,sum(codificado) as codificado,sum(compromiso) as compromiso
,sum(devengado) as devengado,sum(ejecutado) as ejecutado,sum(saldo) as saldo
from (select CUENMC,partida,NOLAAD,inicial,reforma,codificado,compromiso,devengado,ejecutado,(codificado-devengado) as saldo ,nivemc
from (select CUENMC
,(case when nivemc = 1 then SUBSTR(CUENMC, 1, 1)
when nivemc = 2 then SUBSTR(CUENMC, 1, 1)||'.'||SUBSTR(CUENMC, 2, 2)
when nivemc = 3 then SUBSTR(CUENMC, 1, 1)||'.'||SUBSTR(CUENMC, 2, 1)||'.'||SUBSTR(CUENMC, 3, 2)
when nivemc = 4 then SUBSTR(CUENMC, 1, 1)||'.'||SUBSTR(CUENMC, 2, 1)||'.'||SUBSTR(CUENMC, 3, 2)||'.'||SUBSTR(CUENMC, 5, 2)
when nivemc = 5 then SUBSTR(CUENMC, 1, 1)||'.'||SUBSTR(CUENMC, 2, 1)||'.'||SUBSTR(CUENMC, 3, 2)||'.'||SUBSTR(CUENMC, 5, 2)||'.'||SUBSTR(CUENMC, 7, 2)
when nivemc = 6 then SUBSTR(CUENMC, 1, 1)||'.'||SUBSTR(CUENMC, 2, 1)||'.'||SUBSTR(CUENMC, 3, 2)||'.'||SUBSTR(CUENMC, 5, 2)||'.'||SUBSTR(CUENMC, 7, 2)||'.'||SUBSTR(CUENMC, 9, 2) end) as partida
,NOLAAD,nivemc
from USFIMRU.TIGSA_GLM03
where TIPLMC= 'P' and substr(CEDTMC,1,1) in (5,6,7,8,9)
order by CEDTMC,nivemc)
left join(
select CUENDT
,(case when inicial is null then 0 else inicial end) as inicial
,(case when reforma is null then 0 else reforma end) as reforma
,(case when codificado is null then 0 else codificado end) as codificado
,(case when compromiso is null then 0 else compromiso end) as compromiso
,(case when devengado is null then 0 else devengado end) as devengado
,(case when ejecutado is null then 0 else ejecutado end) as ejecutado,postma
from (select CUENDT,sum(MONTDT) as codificado,POSTMA
from (select CUENDT,MONTDT,FDOCDT,POSTMA
from (SELECT CUENDT,AUAD02,MONTDT,FDOCDT
FROM USFIMRU.TIGSA_GLB01
where STATDT='E'
AND CCIADT='CM'
AND SAPRDT>=$P{inicial}
AND SAPRDT<=$P{reforma})
left join
(SELECT CAUXMA,POSTMA FROM USFIMRU.TIGSA_GLM05 where TAUXMA = 'CC'
UNION
SELECT CAUXMA,POSTMA FROM USFIMRU.TIGSA_GLM05 where TAUXMA = 'PY')
on AUAD02 = CAUXMA
where substr(FDOCDT,1,5) <= $P{fechaRe}
and postma  is not null)
group by CUENDT,POSTMA)
left join
(select CUENDT as auxi,sum(MONTDT) as inicial,POSTMA as auxi1
from (select CUENDT,MONTDT,POSTMA
from (SELECT CUENDT,AUAD02,MONTDT,FDOCDT
FROM USFIMRU.TIGSA_GLB01
where STATDT='E'
AND CCIADT='CM'
AND SAPRDT=$P{inicial})
left join
(SELECT CAUXMA,POSTMA FROM USFIMRU.TIGSA_GLM05 where TAUXMA = 'CC'
UNION
SELECT CAUXMA,POSTMA FROM USFIMRU.TIGSA_GLM05 where TAUXMA = 'PY')
on AUAD02 = CAUXMA
where postma  is not null)
group by CUENDT,POSTMA )
on CUENDT = auxi and POSTMA = auxi1
left join
(select CUENDT as auxr,sum(MONTDT) as reforma,POSTMA as auxr1
from (select CUENDT,MONTDT,POSTMA
from (SELECT CUENDT,AUAD02,MONTDT,FDOCDT
FROM USFIMRU.TIGSA_GLB01
where STATDT='E'
AND CCIADT='CM'
AND SAPRDT=$P{reforma}
and substr(FDOCDT,1,5) <= $P{fechaRe})
