GROUP BY CUENDT,POSTMA)
on CUENDT = auxc and POSTMA = auxc1
left join
(select CUENDTD,devengado,POSTMAD,
(case when (max(ROUND(ejecutado,2))over (PARTITION BY substr(CUENDTD,1,2) ORDER BY substr(CUENDTD,1,2))) = ejecutado
