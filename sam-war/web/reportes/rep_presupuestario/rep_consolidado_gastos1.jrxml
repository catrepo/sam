where postma  is not null)
group by CUENDT,POSTMA )
on CUENDT = auxr and POSTMA = auxr1
left join
(select CUENDT as  auxc,sum(compromiso) as compromiso,POSTMA as auxc1
from (SELECT CUENDT , AUAD02 , SUM(MONTDT) as compromiso
FROM USFIMRU.TIGSA_GLB01
where STATDT='E'
AND CCIADT='CM'
AND SAPRDT>=$P{fechai}
AND SAPRDT<=$P{fechaf}
AND CAUXDT='1'
AND TMOVDT='D'
GROUP BY CUENDT, AUAD02)
left join
(SELECT CAUXMA,POSTMA FROM USFIMRU.TIGSA_GLM05 where TAUXMA = 'CC'
