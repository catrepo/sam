	<queryString>
		<![CDATA[select fecha1,p.CUENMC,p.CEDTMC,p.NOLAAD,p.inicial,p.reforma,(case when p.inicial = 0 then (p.inicial-(case when p.reforma<1 then p.reforma*-1 else p.reforma end))*-1 else (p.inicial-(case when p.reforma<1 then p.reforma*-1 else p.reforma end)) end) as codificado,p.AUAD02,p.dep,p.POSTM,p.proyecto
,(case when compromiso is null  then 0 else compromiso end)as compromiso,(case when devengado is null  then 0 else devengado end)as devengado,proyecto1,
(case when valor is null  then 0 else valor end)as valor,NUCODT,NDOCDT
,totalco as saldo_co
,beneficiario
,FECODT
,fecha
,(sum((case when compromiso is null then 0 else (case when compromiso < 0 then compromiso else compromiso end) end)) over (PARTITION BY CUENDT ORDER BY FECODT) -
sum((case when devengado is null then 0 else devengado end)) over (PARTITION BY CUENDT ORDER BY FECODT))as saldo_dev
 from (select CUENMC,CEDTMC,NOLAAD
,(case when inicial is null  then 0 else inicial end)as inicial
,(case when reforma is null  then 0 else reforma end)as reforma
,(case when codificado is null  then 0 else codificado end)as codificado,auad02,dep,postm,proyecto
from (select CUENMC,CEDTMC,NOLAAD
from USFIMRU.TIGSA_GLM03
where TIPLMC= 'P' and substr(CEDTMC,1,1) in (5,6,7,8,9))
inner join (
select CUENDT,inicial,reforma,codificado,AUAD02,dep,POSTM,proyecto
from (select CUENDT ,codificado,AUAD02 ,dep,POSTM
,(select DISTINCT NOMBMA from USFIMRU.TIGSA_GLm05 where CAUXMA = AUAD02 and POSTMA=POSTM ) as proyecto
 from (select *
from(select CUENDT , sum(MONTDT) as codificado,AUAD02,FDOCDT
from (SELECT CUENDT , MONTDT,AUAD02,FDOCDT
FROM USFIMRU.TIGSA_GLB01
where STATDT='E'
AND CCIADT='CM'
AND TMOVDT='D'
AND SAPRDT>=$P{inicial}
AND SAPRDT<=$P{reforma}
AND TAAD02 = 'PY'
AND AUAD02 in (SELECT CAUXMA FROM USFIMRU.TIGSA_GLM05 where TAUXMA = 'PY'))
group by CUENDT ,AUAD02,FDOCDT)
where substr(FDOCDT,1,5) <=$P{ani2})
left join
(SELECT CAUXMA,POSTMA as POSTM,( select NOMBZ1 from USFIMRU.TIGSA_GLM11 where CLASZ1 = '001' and ZONAZ1 = SUBSTR(ZONAMA, 0, 2)) as dep
FROM USFIMRU.TIGSA_GLM05 where TAUXMA = 'PY')
on AUAD02 = CAUXMA)
left join
(SELECT CUENDT as cuentr, sum(MONTDT) as inicial,AUAD02 as auxi
FROM USFIMRU.TIGSA_GLB01
where STATDT='E'
AND CCIADT='CM'
AND TMOVDT='D'
AND SAPRDT=$P{inicial}
AND TAAD02 = 'PY'
AND AUAD02 in (SELECT CAUXMA FROM USFIMRU.TIGSA_GLM05 where TAUXMA = 'PY')
group by CUENDT, AUAD02)
on AUAD02 = auxi
left join
(SELECT CUENDT as cuentr, sum(MONTDT) as reforma,AUAD02 as auxr
FROM USFIMRU.TIGSA_GLB01
where STATDT='E'
AND CCIADT='CM'
AND TMOVDT='D'
AND SAPRDT=$P{reforma}
AND TAAD02 = 'PY'
AND AUAD02 in (SELECT CAUXMA FROM USFIMRU.TIGSA_GLM05 where TAUXMA = 'PY')
group by CUENDT, AUAD02)
on AUAD02 = auxr
where AUAD02 =$P{proyecto})
on CUENMC = CUENDT) p
left join
(select (case when fdocdt is null then null else (substr(fdocdt,6,2)||'/'||substr(fdocdt,4,2)||'/20'||substr(fdocdt,2,2))end)as fecha
,(case when FECODT is null then null else (substr(FECODT,6,2)||'/'||substr(FECODT,4,2)||'/20'||substr(FECODT,2,2))end)as fecha1
,CUENDT,FDOCDT,codificado,compromiso,devengado,AUAD02,NDOCDT,proyecto as proyecto1,valor,NUCODT
,(codificado-sum((case when compromiso is null then 0 else compromiso end)) over (PARTITION BY CUENDT ORDER BY FECODT)) as totalco
,sum((case when devengado is null then 0 else devengado end)) over (PARTITION BY CUENDT ORDER BY FECODT,NUCODT) as totalde
,FECODT
,beneficiario
from (select CUENDT,FDOCDT,codificado,compromiso,null as devengado,AUAD02,NDOCDT,null as proyecto,null as valor,NUCODT, null as beneficiario,FECODT
from (select CUENDT , sum(codificado) as codificado,AUAD02
from (select CUENDT , codificado,AUAD02
from (SELECT CUENDT , sum(MONTDT) as codificado,AUAD02,FDOCDT
FROM USFIMRU.TIGSA_GLB01
where STATDT='E'
AND CCIADT='CM'
AND TMOVDT='D'
AND SAPRDT>=$P{inicial}
AND SAPRDT<=$P{reforma}
AND TAAD02 = 'PY'
AND AUAD02 in (SELECT CAUXMA FROM USFIMRU.TIGSA_GLM05 where TAUXMA = 'PY')
group by CUENDT , AUAD02,FDOCDT)
where substr(FDOCDT,1,5) <=$P{ani2})
group by CUENDT ,AUAD02)
left join (
select * from (
SELECT CUENDT as cuentac, sum(MONTDT) as compromiso,AUAD02 as auxc,FDOCDT,NDOCDT,NUCODT,FECODT
FROM USFIMRU.TIGSA_GLB01
where STATDT='E'
AND CCIADT='CM'
AND TMOVDT='D'
AND SAPRDT>=$P{ani1}
AND SAPRDT<=$P{ani2}
AND CAUXDT='1'
AND TAAD02 = 'PY'
AND AUAD02 in (SELECT CAUXMA FROM USFIMRU.TIGSA_GLM05 where TAUXMA = 'PY')
group by CUENDT,AUAD02 ,FDOCDT,NDOCDT,NUCODT,FECODT))
on CUENDT = cuentac and AUAD02 = auxc
union
select CUENDT,FDOCDT,codificado,null as compromiso,devengado,AUAD02,NDOCDT,proyecto,null as valor,NUCODT, beneficiario,FECODT
from (select CUENDT , sum(codificado) as codificado,AUAD02
from (select CUENDT , codificado,AUAD02
from (SELECT CUENDT , sum(MONTDT) as codificado,AUAD02,FDOCDT
FROM USFIMRU.TIGSA_GLB01
where STATDT='E'
AND CCIADT='CM'
AND TMOVDT='D'
AND SAPRDT>=$P{inicial}
AND SAPRDT<=$P{reforma}
AND TAAD02 = 'PY'
AND AUAD02 in (SELECT CAUXMA FROM USFIMRU.TIGSA_GLM05 where TAUXMA = 'PY')
group by CUENDT , AUAD02,FDOCDT)
where substr(FDOCDT,1,5) <=$P{ani2})
group by CUENDT ,AUAD02)
left join (
select cuentad,FDOCDT,sum(MONTDT) as devengado,auxd,NDOCDT,proyecto,NUCODT
, (case when SUBSTR(proyecto, 0, INSTR(proyecto, '.-')-1) is not null then SUBSTR(proyecto, 0, INSTR(proyecto, '.-')-1)
when SUBSTR(proyecto, 0, INSTR(proyecto, ':')-1) is not null then SUBSTR(proyecto, 0, INSTR(proyecto, ':')-1)
when SUBSTR(proyecto, 0, INSTR(proyecto, ',')-1) is not null then SUBSTR(proyecto, 0, INSTR(proyecto, ',')-1)
when SUBSTR(proyecto, 0, INSTR(proyecto, '.')-1) is not null then SUBSTR(proyecto, 0, INSTR(proyecto, '.')-1)
end) as beneficiario ,FECODT
from (SELECT CUENDT as cuentad, MONTDT,AUAD02 as auxd,FDOCDT,NDOCDT,NUCODT,FECODT
,(select DISTINCT COHEAD from USFIMRU.TIGSA_GLAI3 where CCIAAD=CCIADT and SAPRAD=SAPRDT and TICOAD=TICODT and NUCOAD=NUCODT) as proyecto
FROM USFIMRU.TIGSA_GLB01
where STATDT='E'
AND CCIADT='CM'
AND TMOVDT='D'
AND SAPRDT>=$P{ani1}
AND SAPRDT<=$P{ani2}
AND CAUXDT='2'
AND TAAD02 = 'PY'
AND AUAD02 in (SELECT CAUXMA FROM USFIMRU.TIGSA_GLM05 where TAUXMA = 'PY'))
group by cuentad,auxd,FDOCDT,NDOCDT,proyecto,NUCODT,FECODT)
on CUENDT = cuentad and AUAD02 = auxd
union
select  CUENDT,FDOCDT,codificado,null as compromiso,null as devengado,AUAD02,NDOCDT,proyecto,valor,NUCODT, null as beneficiario ,FECODT
from (select CUENDT , sum(codificado) as codificado,AUAD02
from (select CUENDT , sum(codificado) as codificado,AUAD02
from (select CUENDT , codificado,AUAD02
from (SELECT CUENDT , sum(MONTDT) as codificado,AUAD02,FDOCDT
FROM USFIMRU.TIGSA_GLB01
where STATDT='E'
AND CCIADT='CM'
AND TMOVDT='D'
AND SAPRDT>=$P{inicial}
AND SAPRDT<=$P{reforma}
AND TAAD02 = 'PY'
AND AUAD02 in (SELECT CAUXMA FROM USFIMRU.TIGSA_GLM05 where TAUXMA = 'PY')
group by CUENDT , AUAD02,FDOCDT)
where substr(FDOCDT,1,5) <=$P{ani2})
group by CUENDT ,AUAD02)
group by CUENDT ,AUAD02 )
left join
(select FDOCDT,NDDOAD,sum(valor)as valor,NUCODT,NDOCDT,CDDOAD,cohead as proyecto,FECODT
from (select FDOCDT,NDDOAD,valor,NUCODT,NDOCDT,CDDOAD,cohead,FECODT
from(select FDOCDT,NDDOAD,sum(MONTDT)as valor,NUCODT,NDOCDT,CDDOAD,FECODT,CCIADT,SAPRDT,TICODT
from (select FDOCDT,NDDOAD,MONTDT,NUCODT,NDOCDT,CDDOAD,FECODT,CCIADT,SAPRDT,TICODT
from USFIMRU.TIGSA_GLB01
where STATDT='E'
AND CCIADT='MR'
AND SAPRDT>=$P{ani1}
AND SAPRDT<=$P{ani2}
and CUENDT between '11203%' and '11205%'
and CDDOAD = 'CO')
group by  FDOCDT,NDDOAD,NUCODT,NDOCDT,CDDOAD,FECODT,CCIADT,SAPRDT,TICODT
having sum(MONTDT)<>0)
left join
(SELECT cohead, CCIAAD,SAPRAD,TICOAD,NUCOAD
from USFIMRU.TIGSA_GLAI3 )
on CCIAAD=CCIADT AND SAPRAD=SAPRDT AND TICOAD=TICODT AND NUCOAD=NUCODT)
group by  FDOCDT,NDDOAD,NUCODT,NDOCDT,CDDOAD,cohead,FECODT
having sum(valor)<>0)
on  AUAD02 = NDDOAD)
where AUAD02 = $P{proyecto} and FDOCDT is not null)
on CUENMC=CUENDT]]>
	</queryString>
