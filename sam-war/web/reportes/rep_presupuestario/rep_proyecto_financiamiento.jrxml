	<parameter name="inicial" class="java.lang.Integer"/>
	<parameter name="reforma" class="java.lang.Integer"/>
	<parameter name="ani1" class="java.lang.Integer"/>
	<parameter name="ani2" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select AUAD02,POSTM,CEDTMC,finan,inicial,reforma,compromiso,devengado,codificado
,(codificado-compromiso) as saldo_com
,(compromiso-devengado) as saldo_dev
from (
select AUAD02,POSTM,CEDTMC,finan
,(case when inicial is null then 0 else inicial end)as inicial
,(case when reforma is null then 0 else reforma end)as reforma
,((case when inicial is null then 0 else inicial end)+(case when reforma is null then 0 else reforma end)) as codificado
,(case when compromiso is null then 0 else compromiso end)as compromiso
,(case when devengado is null then 0 else devengado end)as devengado
from (select CUENMC,CEDTMC,NOLAAD
from USFIMRU.TIGSA_GLM03
where TIPLMC= 'P' and substr(CEDTMC,1,1) in (5,6,7,8,9))
inner join(
select CUENDT,inicial,AUAD02,POSTM,finan,AUAD01
from(
SELECT CUENDT, MONTDT as inicial,AUAD02,AUAD01
,(select DISTINCT NOMBMA from USFIMRU.TIGSA_GLm05 where CAUXMA = AUAD01 and TAUXMA = TAAD01) as finan
FROM USFIMRU.TIGSA_GLB01
where STATDT='E'
AND CCIADT='CM'
AND TMOVDT='D'
AND SAPRDT=$P{inicial}
AND TAAD02 = 'PY')
left join
(SELECT CAUXMA,POSTMA  as POSTM FROM USFIMRU.TIGSA_GLM05 where TAUXMA = 'PY')
on AUAD02 = CAUXMA)
on CUENMC = CUENDT
left join
(SELECT CUENDT as cuentar, MONTDT as reforma,AUAD02 as aux,AUAD01 as aux1
FROM USFIMRU.TIGSA_GLB01
where STATDT='E'
AND CCIADT='CM'
AND TMOVDT='D'
AND SAPRDT=$P{reforma}
AND TAAD02 = 'PY')
on CUENMC =cuentar and AUAD02= aux and AUAD01=aux1
left join
(SELECT CUENDT as cuentac, sum(MONTDT) as compromiso,AUAD02 as auxc,AUAD01 as auxc1
FROM USFIMRU.TIGSA_GLB01
where STATDT='E'
AND CCIADT='CM'
AND TMOVDT='D'
AND SAPRDT>= $P{ani1}
AND SAPRDT<= $P{ani2}
AND CAUXDT='1'
AND TAAD02 = 'PY'
group by CUENDT,AUAD02,AUAD01)
on CUENMC =cuentac and AUAD02= auxc and AUAD01= auxc1
left join
(SELECT CUENDT as cuentad, sum(MONTDT) as devengado,AUAD02 as auxd,AUAD01 as auxd1
FROM USFIMRU.TIGSA_GLB01
where STATDT='E'
AND CCIADT='CM'
AND TMOVDT='D'
AND SAPRDT>= $P{ani1}
AND SAPRDT<= $P{ani2}
AND CAUXDT='2'
AND TAAD02 = 'PY'
group by CUENDT,AUAD02,AUAD01)
on CUENMC =cuentad and AUAD02= auxd and AUAD01= auxd1)
order by finan]]>
	</queryString>
