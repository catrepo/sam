doc_numero,
doc_responsabe,
doc_concepto,
doc_fecharev,
doc_valor,
doc_fechacon,
(case when doc_revisioncon = 't' then 'Atendido' when doc_revisiondev = 't' then 'De Vuelto' end ),
doc_comprobante,
doc_dependencia,
doc_usuasignacion
FROM
