	<queryString>
		<![CDATA[SELECT
id_documento,
doc_fecharev,
(SELECT tipo_nombre FROM tes_tipo_documento where id_tipo =tes_documentos.id_tipo) AS tipo,
doc_numero,
doc_responsabe,
doc_concepto,
doc_fecharev,
doc_valor,
doc_fechacon,
(case when doc_revisioncon = 't' then 'Atendido' when doc_revisiondev = 't' then 'De Vuelto' end ),
doc_comprobante,
doc_dependencia,
doc_usuasignacion
FROM
tes_documentos
where (doc_revisioncon = 't' or doc_revisiondev ='t') and doc_usuasignacion =$P{usuario}
order by tipo,(case when doc_revisioncon = 't' then 'Atendido' when doc_revisiondev = 't' then 'De Vuelto' end )]]>
