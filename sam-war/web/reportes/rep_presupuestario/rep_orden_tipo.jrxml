	<queryString>
		<![CDATA[SELECT
tes_ide_orden_pago,
tes_numero_orden,
(case when tes_id_proveedor is null then tes_empleado else tes_proveedor end) as beneficiario,
tes_valor,
tes_concepto,
tes_acuerdo,
tes_nota,
tes_fecha_ingreso,
tes_mes,
tes_anio,
tes_estado,
tes_fecha_anu,
tes_comentario_anula
FROM tes_orden_pago
where tes_estado like $P{tipo} and tes_anio like $P{anio}
order by tes_numero_orden]]>
	</queryString>
